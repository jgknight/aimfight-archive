class com.robertpenner.easing.Linear
{
   function Linear()
   {
   }
   static function easeNone(t, b, c, d)
   {
      return c * t / d + b;
   }
   static function easeIn(t, b, c, d)
   {
      return c * t / d + b;
   }
   static function easeOut(t, b, c, d)
   {
      return c * t / d + b;
   }
   static function easeInOut(t, b, c, d)
   {
      return c * t / d + b;
   }
   static function easeOutIn(t, b, c, d)
   {
      return c * t / d + b;
   }
}
